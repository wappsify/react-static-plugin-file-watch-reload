{
  "name": "react-static-plugin-file-watch-reload",
  "version": "1.0.3",
  "description": "A react-static plugin for development environments triggering route rebuilds when files change",
  "keywords": [
    "react-static",
    "plugin",
    "file",
    "watch",
    "reload",
    "data",
    "change"
  ],
  "main": "dist/node.api.js",
  "author": "Yannik Wissner <hello@yannik.im>",
  "repository": "https://github.com/wappsify/react-static-plugin-file-watch-reload",
  "license": "MIT",
  "private": false,
  "scripts": {
    "watch": "yarn build --watch",
    "prepare": "yarn build",
    "build": "babel src --out-dir './dist' --extensions '.ts'"
  },
  "peerDependencies": {
    "react-static": "^7.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.10.5",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "react-static": "7.4.2"
  },
  "dependencies": {
    "chokidar": "3.4.1"
  }
}
